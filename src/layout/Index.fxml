<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="IndexBack" onDragDetected="#StartGame" prefHeight="900.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="layout.MainController">
   <children>
      <Button fx:id="NewGame" layoutX="54.0" layoutY="821.0" mnemonicParsing="false" onAction="#StartGame" prefHeight="38.0" prefWidth="158.0" text="새 게임">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="LoadGame" layoutX="342.0" layoutY="821.0" mnemonicParsing="false" onAction="#LoadGame" prefHeight="38.0" prefWidth="158.0" text="게임 불러오기">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="EndGame" layoutX="888.0" layoutY="821.0" mnemonicParsing="false" onAction="#ChangeStart" prefHeight="38.0" prefWidth="158.0" text="게임 종료">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="Setting" layoutX="622.0" layoutY="821.0" mnemonicParsing="false" onAction="#ChangeSetting" prefHeight="38.0" prefWidth="158.0" text="설정">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
