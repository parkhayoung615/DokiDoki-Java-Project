<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="900.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="layout.MainController">
   <children>
      <ImageView fitHeight="900.0" fitWidth="1100.0" pickOnBounds="true">
         <image>
            <Image url="@../imgs/GameOver.png" />
         </image>
      </ImageView>
      <Button fx:id="ReStartBtn" layoutX="286.0" layoutY="630.0" mnemonicParsing="false" onAction="#ReStart" prefHeight="48.0" prefWidth="529.0" text="새로 시작하기" />
      <Button fx:id="ReLoadBtn" layoutX="286.0" layoutY="710.0" mnemonicParsing="false" onAction="#ReLoad" prefHeight="48.0" prefWidth="529.0" text="불러오기" />
      <Button fx:id="LifeEndBtn" layoutX="286.0" layoutY="787.0" mnemonicParsing="false" onAction="#EndLife" prefHeight="48.0" prefWidth="529.0" text="인생 종료하기 ㅋㅋ 그걸지냐" />
   </children>
</AnchorPane>
