<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="layout.MainController">
   <children>
      <TableView layoutX="30.0" layoutY="22.0" prefHeight="753.0" prefWidth="1040.0">
        <columns>
          <TableColumn prefWidth="97.0" text="번호" />
          <TableColumn prefWidth="236.0" text="보유 프로그래밍 언어" />
            <TableColumn prefWidth="480.0" text="HP" />
            <TableColumn prefWidth="226.0" text="마지막 세이브 위치" />
        </columns>
      </TableView>
      <Button fx:id="NewGame" layoutX="54.0" layoutY="821.0" mnemonicParsing="false" onAction="#StartGame" prefHeight="38.0" prefWidth="158.0" text="새 게임">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="LoadGame" layoutX="342.0" layoutY="821.0" mnemonicParsing="false" onAction="#LoadGame" prefHeight="38.0" prefWidth="158.0" text="게임 불러오기">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="EndGame" layoutX="888.0" layoutY="821.0" mnemonicParsing="false" onAction="#ChangeStart" prefHeight="38.0" prefWidth="158.0" text="게임 종료">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="Setting" layoutX="622.0" layoutY="821.0" mnemonicParsing="false" onAction="#ChangeSetting" prefHeight="38.0" prefWidth="158.0" text="설정">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>

